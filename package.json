{
  "name": "coffeelint-teamcity",
  "version": "0.2.0",
  "description": "TeamCity reporter for CoffeeLint",
  "main": "lib/reporter.js",
  "scripts": {
    "compile": "./node_modules/.bin/coffee -o ./lib ./src",
    "pretest": "npm run compile",
    "test": "./node_modules/.bin/coffeelint ./src && mocha --compilers coffee:coffee-script/register",
    "prepublish": "npm run compile"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/aranasoft/coffeelint-teamcity"
  },
  "keywords": [
    "teamcity",
    "coffeelint",
    "reporter",
    "validate",
    "lint"
  ],
  "author": "Arana Software <info@aranasoft.com> (http://www.aranasoft.com)",
  "contibutors": [
    "Dennis Burton <dburton@aranasoft.com>",
    "Jay Harris <jay@aranasoft.com>"
  ],
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/aranasoft/coffeelint-teamcity/issues"
  },
  "homepage": "https://github.com/aranasoft/coffeelint-teamcity",
  "devDependencies": {
    "coffee-script": "^1.9.1",
    "coffeelint": "^1.4",
    "mocha": "^2",
    "should": "^5"
  }
}
